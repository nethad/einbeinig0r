<h1>Listing participants</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Born in year</th>
    <th>Gender</th>
    <th>Topic keywords</th>
    <th>Topic description</th>
    <th>Skype user</th>
    <th>Gmail address</th>
    <th>Sip address</th>
    <th>Phone number</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @participants.each do |participant| %>
  <tr>
    <td><%= participant.name %></td>
    <td><%= participant.email %></td>
    <td><%= participant.born_in_year %></td>
    <td><%= participant.gender %></td>
    <td><%= participant.topic_keywords %></td>
    <td><%= participant.topic_description %></td>
    <td><%= participant.skype_user %></td>
    <td><%= participant.gmail_address %></td>
    <td><%= participant.sip_address %></td>
    <td><%= participant.phone_number %></td>
    <td><%= link_to 'Show', participant %></td>
    <td><%= link_to 'Edit', edit_participant_path(participant) %></td>
    <td><%= link_to 'Destroy', participant, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Participant', new_participant_path %>
